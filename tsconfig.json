  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./build"
  },
  "include": ["server.ts", "routes", "models"],
  "exclude": ["node_modules"]
}